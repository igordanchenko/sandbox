{
    "include": [
        "src"
    ],
    "compilerOptions": {
        "outDir": "dist",
        "allowJs": true,
        "jsx": "react",
        "declaration": true,
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "NodeNext",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    }
}
